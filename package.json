{
  "name": "hello-doctor",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "dependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.61",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "axios": "^1.6.0",
    "dayjs": "^1.11.10",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.18.0",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "styled-components": "^6.1.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "test": "react-app-rewired test --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "testNoWatch": "react-app-rewired test --watchAll=false --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "eject": "react-app-rewired eject",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --jsx-single-quote --single-quote --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --jsx-single-quote --single-quote --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@cfaester/enzyme-adapter-react-18": "^0.7.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "https-browserify": "^1.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.2",
    "os-browserify": "^0.3.0",
    "prettier": "^3.0.3",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "react-test-renderer": "^18.2.0",
    "redux-mock-store": "^1.5.4",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.3"
  }
}
